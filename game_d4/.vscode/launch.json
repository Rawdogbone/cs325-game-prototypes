{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "chrome",
			"request": "attach",
			"name": "Attach to Chrome",
			"port": 9222,
			"webRoot": "${workspaceFolder}"
		},
		{
			"type": "chrome",
			"request": "launch",
			"runtimeExecutable": "/usr/bin/google-chrome-beta", // or whatever you chrome's application path is
			"name": "Launch Chrome against localhost",
			"url": "http://localhost:8000",
			"webRoot": "${workspaceFolder}"
		}
	]
}